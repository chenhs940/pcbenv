	axlfcreate("SILK_TOP" L_Num SilkList)
	
	SilkList = '("BOARD GEOMETRY/OUTLINE"  
		"PACKAGE GEOMETRY/SILKSCREEN_BOTTOM" "BOARD GEOMETRY/SILKSCREEN_BOTTOM"
	)
