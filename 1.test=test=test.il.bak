			)
			when(IDPinShape->objType == "shape" && IDViaCline->net->name == "GND"
				axlDRCWaive(t n0)
			)
			when(IDPinShape->objType == "pin" && IDViaCline->net->name == IDPinShape->net->name
				axlDRCWaive(t n0)
			)
		)
	)
	t
)
