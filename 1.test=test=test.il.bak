procedure( DrcAuto_Sold2PadClineSpacing()
let(()
	axlDBRefreshId(nil)
	foreach(n0 axlDBGetDesign()->drcs
		when(n0->name == "Soldermask to VIA Spacing"
			foreach(n1 n0->violations
				when(n1->objType == "via" && n1->net->name == "GND"
					axlDRCWaive(t n0)
				)
			)
		)
