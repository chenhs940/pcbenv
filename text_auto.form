FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 45 30
HEADER "自动调丝印  V1.0 "

POPUP <silk_layer>		 		"TOP" "TOP",		 		"BOTTOM" "BOTTOM"	 		.

TILE
GROUP "选择丝印的层面:"
GLOC 5 1
GSIZE 22 5
ENDGROUP
FIELD silk_layer
FLOC 7 3
ENUMSET 16
POP "silk_layer"
ENDFIELD
GROUP "选择字体:"
GLOC 30 1
GSIZE 8 5
ENDGROUP
FIELD silk_size
FLOC 32 3
INTSLIDEBAR 2 2
MIN 1
MAX 50
ENDFIELD
GROUP "信息:"
GLOC 1 6
GSIZE 42 6
ENDGROUP
TEXT
TLOC 3 8
INFO silk_info 60
ENDTEXT
TEXT
TLOC 15 10
INFO sinfo 60
OPTIONS underline BOLD
ENDTEXT
FIELD silk_auto_ok
FLOC 10 13
MENUBUTTON "开始 " 10 4
ENDFIELD
FIELD aquit
FLOC 30 13
MENUBUTTON "Exit" 10 4
ENDFIELD
FIELD update_bar
FLOC 3 16
PROGRESS 40 3
ENDFIELD

ENDTILE

ENDFORM
