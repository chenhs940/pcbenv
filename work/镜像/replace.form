FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 45 30
HEADER "旋转&镜像"
POPUP <operation>		 		"旋转" "XUAN",		 	"移动" "YI",		 		"镜像" "JING"	 		.

TILE

GROUP "操作方式"
GLOC 1 1
GSIZE 40 5
ENDGROUP

FIELD operation
FLOC 2 3
ENUMSET 10
POP "operation"
ENDFIELD

TEXT	"e.g:"
TLOC 15 3
INFO my_info 60
ENDTEXT

FIELD argc
FLOC 25 3      
STRFILLIN 12 15  
ENDFIELD


TEXT "Select Pin:"
TLOC 2 25
ENDTEXT

FIELD Select_Pin
FLOC 10 25      
STRFILLIN 20 20  
ENDFIELD

FIELD Select
FLOC 35 25
MENUBUTTON "Select" 10 3
ENDFIELD


FIELD OK
FLOC 15 29
MENUBUTTON "OK" 10 3
ENDFIELD


ENDTILE
ENDFORM
